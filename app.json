{
  "expo": {
    "name": "Kmon",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/kmon2.png",
    "splash": {
      "image": "./assets/images/kmon2.png",
      "resizeMode": "contain",
      "backgroundColor": "#382020",
      "dark": {
      "backgroundColor": "#382020"
      }
    },
    "scheme": "app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "splash": {
        "backgroundColor": "#382020"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#382020",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/kmon2.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/kmon2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#382020",
          "dark": {
            "backgroundColor": "#382020"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
